{
    "name": "marmelab/silrest",
    "description": "A silex provider to easely setup a rest api with silex.",
    "keywords": ["silex", "rest", "api"],
    "type": "provider",
    "require": {
        "silex/silex": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2.0",
        "symfony/browser-kit": "~2.3.0",
        "symfony/browser-kit": ">=2.3,<2.4-dev",
        "symfony/css-selector": ">=2.3,<2.4-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alexis",
            "email": "alexis@marmelab.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Marmelab\\Silex\\Provider\\Silrest\\": "src/",
            "Marmelab\\Silex\\Provider\\SilrestTest\\": "tests/"
        }
    }
}
